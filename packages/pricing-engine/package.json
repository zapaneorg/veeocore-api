{
  "name": "@veeo/pricing-engine",
  "version": "1.0.0",
  "description": "Moteur de calcul des prix pour VTC/Taxi",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "pricing",
    "vtc",
    "taxi",
    "fare-calculator"
  ],
  "author": "VeeoStras",
  "license": "UNLICENSED",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
